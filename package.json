{
	"name": "webpack-babel-config-react",
	"version": "1.0.0",
	"description": "",
	"main": "dist/main.js",
	"scripts": {
		"start": "node dist/main.js",
		"start:webp": "webpack serve --open",
		"build": "rm -rf dist build && webpack --config webpack.server.js && webpack --config webpack.client.prod.js --mode production --progress",
		"build:dev": "rm -rf dist build && webpack --config webpack.server.js && webpack --config webpack.client.dev.js --mode development",
		"dev": "yarn build:dev && npm-run-all --parallel dev:*",
		"dev:build-server": "webpack --config webpack.server.js --watch",
		"dev:build-client": "webpack --config webpack.client.dev.js --watch",
		"dev:server": "nodemon --watch dist --exec \"node dist/main.js\""
	},
	"author": "Priya Bihani",
	"license": "ISC",
	"dependencies": {
		"axios": "^0.21.1",
		"bootstrap": "^4.5.0",
		"express": "^4.17.1",
		"file-loader": "^6.2.0",
		"framer-motion": "^1.10.3",
		"highlight.js": "^10.0.3",
		"jquery": "^3.5.1",
		"moment": "^2.29.1",
		"npm-run-all": "^4.1.5",
		"popper.js": "^1.16.1",
		"react": "^17.0.1",
		"react-bootstrap": "^1.0.0",
		"react-dom": "^17.0.1",
		"react-helmet": "^6.0.0",
		"react-html-parser": "^2.0.2",
		"react-loadable": "^5.5.0",
		"react-quill": "^1.3.3",
		"react-redux": "^7.2.1",
		"react-router-dom": "^5.2.0",
		"react-scripts": "4.0.1",
		"react-search-autocomplete": "^5.2.2",
		"react-select": "^4.3.1",
		"react-tippy": "^1.4.0",
		"react-toastify": "^7.0.3",
		"redux": "^4.0.5",
		"redux-devtools-extension": "^2.13.8",
		"redux-thunk": "^2.3.0",
		"web-vitals": "^0.2.4"
	},
	"devDependencies": {
		"@babel/core": "^7.14.3",
		"@babel/preset-env": "^7.14.2",
		"@babel/preset-react": "^7.13.13",
		"babel-loader": "^8.2.2",
		"compression-webpack-plugin": "^8.0.0",
		"css-loader": "^5.2.5",
		"html-webpack-plugin": "^5.3.1",
		"nodemon": "^2.0.7",
		"reload": "^3.1.1",
		"style-loader": "^2.0.0",
		"webpack": "^5.37.1",
		"webpack-bundle-analyzer": "^4.4.2",
		"webpack-cli": "^4.7.0",
		"webpack-dev-server": "^3.11.2",
		"webpack-merge": "^5.7.3",
		"webpack-node-externals": "^3.0.0"
	}
}
